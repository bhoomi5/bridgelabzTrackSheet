    "moleculer-repl": "^0.5.7"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "ioredis": "^4.14.0",
    "jsonwebtoken": "^9.0.0",
    "moleculer": "^0.13.9",
    "moleculer-db": "^0.8.4",
    "moleculer-db-adapter-mongo": "^0.4.6",
    "moleculer-db-adapter-mongoose": "^0.8.3",
    "moleculer-web": "^0.8.5",
    "mongoose": "^5.7.12",
    "nats": "^1.3.0",
    "redis": "^2.8.0",
    "redlock": "^4.1.0"
